{
	"entrypoint": "index.html",
	"shell": "src/appod-app.html",
	"fragments": [
		"src/appod-pictures.html",
		"src/appod-loved.html",
		"src/appod-404.html"
  ],
  "sources": [
	  "src/**/*",
	  "images/**/*"
  ],
  "extraDependencies": [
	  "manifest.json",
	  "bower_components/webcomponentsjs/*.js",
	"https://apod.nasa.gov/*"
  ],
  "lint": {
	  "rules": ["polymer-2"]
  },
  "builds": [
	  {
		  "preset": "es5-bundled"
	  },
	  {
		  "preset": "es6-bundled"
	  },
	  {
		  "preset": "es6-unbundled"
	  }
  ]
}
